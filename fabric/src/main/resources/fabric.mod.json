{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",

  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "BlayTheNinth",
    "ZerotS"
  ],
  "contact": {
    "homepage": "${homepage}",
    "sources": "${sources}",
    "issues": "${issues}"
  },

  "license": "${license}",
  "icon": "${mod_id}.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.blay09.mods.cookingforblockheads.fabric.FabricCookingForBlockheads"
    ],
    "client": [
      "net.blay09.mods.cookingforblockheads.fabric.client.FabricCookingForBlockheadsClient"
    ],
    "fabric-datagen": [
      "net.blay09.mods.cookingforblockheads.fabric.datagen.ModDataGenerator"
    ],
    "modmenu": [
      "net.blay09.mods.cookingforblockheads.fabric.compat.ModMenuIntegration"
    ],
    "jade": [
      "net.blay09.mods.cookingforblockheads.fabric.compat.FabricJadeCookingForBlockheadsPlugin"
    ]
  },
  "mixins": [
    "cookingforblockheads.mixins.json",
    "cookingforblockheads.fabric.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14",
    "fabric-api": "*",
    "balm-fabric": "*",
    "minecraft": ">=1.20.2",
    "java": ">=17"
  },
  "suggests": {
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/VAfZ2Nau6j"
      }
    },
    "waila:plugins": {
      "id": "cookingforblockheads:wthit",
      "initializer": "net.blay09.mods.cookingforblockheads.compat.WTHITCookingForBlockheadsPlugin"
    }
  }
}
